{
  "name": "@semiont/demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Demo scripts for Semiont",
  "scripts": {
    "pro-bo": "dotenv -e .env -- tsx pro_bo.ts",
    "freelaw-nh": "dotenv -e .env -- tsx freelaw_nh.ts",
    "demo:citizens-united:download": "dotenv -e .env -- tsx demo.ts citizens_united download",
    "demo:citizens-united:load": "dotenv -e .env -- tsx demo.ts citizens_united load",
    "demo:citizens-united:annotate": "dotenv -e .env -- tsx demo.ts citizens_united annotate",
    "demo:hiking:load": "dotenv -e .env -- tsx demo.ts hiking load",
    "demo:hiking:annotate": "dotenv -e .env -- tsx demo.ts hiking annotate",
    "demo:arxiv:download": "dotenv -e .env -- tsx demo.ts arxiv download",
    "demo:arxiv:load": "dotenv -e .env -- tsx demo.ts arxiv load",
    "arxiv": "dotenv -e .env -- tsx demo.ts arxiv load"
  },
  "dependencies": {
    "@semiont/api-client": "*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "dotenv-cli": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  }
}
