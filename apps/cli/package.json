{
  "name": "@semiont/cli",
  "version": "0.2.24",
  "description": "Semiont CLI - Unified environment management tool",
  "_comment": "AWS SDK dependencies (@aws-sdk/*) are only used by platforms/aws",
  "type": "module",
  "bin": {
    "semiont": "./dist/cli.mjs"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bundledDependencies": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-AI-Alliance/semiont.git",
    "directory": "apps/cli"
  },
  "keywords": [
    "semiont",
    "cli",
    "environment-management",
    "devops",
    "deployment",
    "infrastructure"
  ],
  "author": "Semiont Team",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run typecheck && node build.mjs",
    "build:only": "node build.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "vitest --watch",
    "test:watch:unit": "vitest --watch --config vitest.config.unit.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@aws-sdk/client-cloudformation": "^3.600.0",
    "@aws-sdk/client-cloudwatch": "^3.600.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.600.0",
    "@aws-sdk/client-cost-explorer": "^3.600.0",
    "@aws-sdk/client-ecr": "^3.600.0",
    "@aws-sdk/client-ecs": "^3.600.0",
    "@aws-sdk/client-efs": "^3.600.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.859.0",
    "@aws-sdk/client-lambda": "^3.600.0",
    "@aws-sdk/client-neptune": "^3.600.0",
    "@aws-sdk/client-rds": "^3.600.0",
    "@aws-sdk/client-route-53": "^3.864.0",
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "@aws-sdk/client-sts": "^3.859.0",
    "@aws-sdk/client-wafv2": "^3.859.0",
    "@prisma/client": "^6.13.0",
    "@semiont/api-client": "^0.2.24",
    "@semiont/core": "^0.2.24",
    "@testcontainers/postgresql": "^11.5.1",
    "arg": "^5.0.2",
    "bcrypt": "^5.1.1",
    "commander": "^14.0.0",
    "express": "^4.18.2",
    "ink": "^4.4.1",
    "is-ci": "^3.0.1",
    "is-upper-case": "^2.0.2",
    "js-yaml": "^4.1.0",
    "neo4j-driver": "^5.0.0",
    "openai": "^5.21.0",
    "react": "^18.3.1",
    "simple-git": "^3.27.0",
    "socket.io": "^4.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ink": "^0.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.0.0",
    "@types/yargs": "^17.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^5.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "yargs": "^17.7.0"
  },
  "overrides": {
    "esbuild": "0.27.2"
  }
}
