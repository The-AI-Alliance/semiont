{
  "name": "@semiont/api-types",
  "version": "0.1.0",
  "description": "Shared API type definitions for Semiont frontend and backend",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "types",
    "api",
    "typescript",
    "semiont"
  ],
  "author": "Semiont Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js",
      "require": "./dist/auth.js"
    },
    "./user": {
      "types": "./dist/user.d.ts",
      "import": "./dist/user.js",
      "require": "./dist/user.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js",
      "require": "./dist/common.js"
    }
  }
}