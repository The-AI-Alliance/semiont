{
  "name": "@semiont/inference",
  "version": "0.2.29",
  "type": "module",
  "description": "AI inference capabilities for entity extraction, text generation, and resource creation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run typecheck && tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@semiont/api-client": "*",
    "@semiont/core": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "inference",
    "entity-extraction",
    "llm",
    "anthropic",
    "text-generation"
  ],
  "author": "The AI Alliance",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-AI-Alliance/semiont.git",
    "directory": "packages/inference"
  }
}
