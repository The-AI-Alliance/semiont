{
  "_comment": "Local development environment",
  "deployment": {
    "default": "process"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "services": {
    "frontend": {
      "command": "npm run dev"
    },
    "backend": {
      "command": "npm run dev"
    },
