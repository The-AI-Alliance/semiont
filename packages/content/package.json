    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@semiont/core": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "content",
    "storage",
    "representation",
    "content-addressed",
    "deduplication",
    "semiont"
  ],
  "author": "The AI Alliance",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-AI-Alliance/semiont.git",
    "directory": "packages/content"
  }
}
