{
  "name": "@semiont/content",
  "version": "0.2.30",
  "type": "module",
  "description": "Content-addressed storage for resource representations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run typecheck && tsup",
