{
  "name": "@semiont/api-client",
  "version": "0.1.0",
  "description": "Generated API client from OpenAPI spec",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "openapi-typescript ../../apps/backend/public/openapi.json -o src/types.ts",
    "build": "tsc",
    "prebuild": "npm run generate",
    "watch": "tsc --watch",
    "clean": "rm -rf dist src/types.ts"
  },
  "dependencies": {
    "ky": "^1.2.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "orval": "^6.31.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "semiont",
    "api",
    "client",
    "openapi"
  ],
  "author": "Semiont Team",
  "license": "Apache-2.0"
}
