{
  "name": "@semiont/ontology",
  "version": "0.2.31",
  "type": "module",
  "description": "Entity types, tag schemas, and W3C annotation vocabularies",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run typecheck && tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@semiont/api-client": "*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ontology",
    "entity-types",
    "tag-schemas",
    "w3c",
    "annotations",
    "vocabularies"
  ],
  "author": "The AI Alliance",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-AI-Alliance/semiont.git",
    "directory": "packages/ontology"
  }
}
