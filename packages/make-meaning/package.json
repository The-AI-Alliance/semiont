{
  "name": "@semiont/make-meaning",
  "version": "0.2.32",
  "type": "module",
  "description": "Making meaning from resources through context assembly, pattern detection, and relationship reasoning",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run typecheck && tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@semiont/core": "*",
    "@semiont/api-client": "*",
    "@semiont/event-sourcing": "*",
    "@semiont/content": "*",
    "@semiont/graph": "*",
    "@semiont/ontology": "*",
    "@semiont/inference": "*",
    "@semiont/jobs": "*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "semantic-annotation",
    "knowledge-graph",
    "context-assembly",
    "meaning-making",
    "resource-analysis"
  ],
  "author": "The AI Alliance",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-AI-Alliance/semiont.git",
    "directory": "packages/make-meaning"
  }
}
