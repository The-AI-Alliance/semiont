{
  "_comment": "Production Environment Configuration - Production-specific overrides and settings",
  "deployment": {
    "default": "aws"
  },
  "site": {
    "_comment": "Production-specific site configuration (example values - MUST BE CUSTOMIZED)",
    "domain": "wiki.example.com",
    "adminEmail": "admin@example.com",
    "supportEmail": "support@example.com", 
    "oauthAllowedDomains": ["example.com"]
  },
  "app": {
    "features": {
      "enableAnalytics": true,
      "enableMaintenanceMode": false,
      "enableDebugLogging": false
    },
    "security": {
      "sessionTimeout": 28800,
      "_comment_sessionTimeout": "8 hours",
      "maxLoginAttempts": 5
    },
    "performance": {
      "enableCaching": true,
      "cacheTimeout": 300,
      "_comment_cacheTimeout": "5 minutes"
    }
  },
  "services": {
    "backend": {
      "deployment": {
        "type": "aws"
      },
      "command": "npm start",
      "port": 3001
    },
    "frontend": {
      "deployment": {
        "type": "aws"
      },
      "command": "npm start",
      "port": 3000
    },
    "database": {
      "deployment": {
        "type": "aws"
      },
      "name": "semiont_prod",
      "multiAZ": true,
      "backupRetentionDays": 7
    }
  },
  "cloud": {
    "aws": {
      "stacks": {
        "infra": "SemiontInfraStack",
        "app": "SemiontAppStack"
      }
    }
  },
    "aws": {
      "_comment": "Production-specific AWS configuration (example values - MUST BE CUSTOMIZED)",
      "region": "us-east-2",
      "accountId": "123456789012",
      "certificateArn": "arn:aws:acm:us-east-1:123456789012:certificate/12345678-1234-1234-1234-123456789012",
      "hostedZoneId": "Z1234567890ABC",
      "rootDomain": "example.com",
      "ecs": {
        "desiredCount": 2,
        "minCapacity": 2,
        "maxCapacity": 10
      },
      "monitoring": {
        "enableDetailedMonitoring": true,
        "logRetentionDays": 30
      }
    }
}