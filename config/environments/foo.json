{
  "_comment": "Foo Environment Configuration - Custom environment for testing feature X",
  "_extends": "integration",
  "_characteristics": [
    "Real database (PostgreSQL in Testcontainers)",
    "Custom settings for foo feature testing",
    "Extended timeouts for complex workflows"
  ],
  "site": {
    "domain": "foo.test.example.com"
  },
  "app": {
    "backend": {
      "url": "http://localhost:3001",
      "database": {
        "name": "semiont_foo_test",
        "user": "foo_test_user",
        "password": "foo_test_password"
      }
    },
    "features": {
      "enableDebugLogging": true,
      "enableAnalytics": false
    },
    "security": {
      "sessionTimeout": 10800,
      "_comment_sessionTimeout": "3 hours for extended foo workflows",
      "maxLoginAttempts": 15
    }
  }
}