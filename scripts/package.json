{
  "name": "semiont-scripts",
  "version": "1.0.0",
  "description": "TypeScript-based management scripts for Semiont on AWS",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "logs": "node dist/logs.js",
    "status": "node dist/status.js",
    "secrets": "node dist/secrets.js",
    "restart": "node dist/restart.js",
    "exec": "node dist/exec.js",
    "maintenance": "node dist/maintenance.js",
    "backup": "node dist/db-backup.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.600.0",
    "@aws-sdk/client-cloudwatch": "^3.600.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.600.0",
    "@aws-sdk/client-cost-explorer": "^3.600.0",
    "@aws-sdk/client-ecr": "^3.600.0",
    "@aws-sdk/client-ecs": "^3.600.0",
    "@aws-sdk/client-efs": "^3.600.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.848.0",
    "@aws-sdk/client-rds": "^3.600.0",
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "@aws-sdk/client-sts": "^3.859.0",
    "@aws-sdk/client-wafv2": "^3.848.0",
    "@prisma/client": "^6.13.0",
    "@testcontainers/postgresql": "^11.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-cdk-lib": "^2.208.0",
    "commander": "^14.0.0",
    "constructs": "^10.4.2",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/react": "^18",
    "@types/yargs": "^17.0.0",
    "chalk": "^5.3.0",
    "ink": "^4.4.1",
    "react": "^18.3.1",
    "typescript": "^5.0.0",
    "yargs": "^17.7.0"
  }
